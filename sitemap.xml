---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for page in site.pages %}
        <url>
            <loc>{{ site.url }}{{ page.url }}</loc>
            {% if page.date %}<lastmod>{{ page.date | date_to_xmlschema }}</lastmod>{% else %}<lastmod>{{ site.time | date_to_xmlschema }}</lastmod>{% endif %}
            <changefreq>weekly</changefreq>
            <priority>1.0</priority>
        </url>
    {% endfor %}
    {% for post in site.posts %}
        <url>
            <loc>{{ site.url }}{{ post.url }}</loc>
            <lastmod>{% if post.lastmod == null %}{{ post.date | date_to_xmlschema }}{% else %}{{ post.lastmod | date_to_xmlschema }}{% endif %}</lastmod>
            <changefreq>weekly</changefreq>
            <priority>1.0</priority>
        </url>
    {% endfor %}
</urlset>
